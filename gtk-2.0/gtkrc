	bg[PRELIGHT]	= mix(0.93, shade (1.11,@bg_color), shade (1.04,@selected_bg_color))
	bg[ACTIVE]      = shade (0.80, @bg_color)
	bg[INSENSITIVE] = shade (1.055, @bg_color)
	
	engine "aurora" {
		curvature	= 3.0
		old_arrowstyle  = FALSE
	}
}

