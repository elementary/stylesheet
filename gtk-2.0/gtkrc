style "notebook-inverse" = "murrine-notebook"
{

	xthickness = 0
	ythickness = 0

	engine "pixmap" 
	{
		#This is the image used for the selected tab.
		image 
		{
			function 	= EXTENSION
			state 		= NORMAL
			gap_side 	= BOTTOM
			file 		= "Tabs/ActiveTab.png"
			border 		= {10,10,20,0}
			stretch 	= TRUE
		}

		#This is the image used for tabs in the background.
		image 
		{
			function 	= EXTENSION
			state 		= ACTIVE
			gap_side 	= BOTTOM
			file 		= "Others/null.png"
			border 		= {10,10,20,0}
			stretch 	= TRUE
		}
	}
}

