#: data/stylesheet.appdata.xml.in:8
msgid "System Stylesheet"
msgstr "Systemowy arkusz stylu"

#: data/stylesheet.appdata.xml.in:9
msgid "The look and feel for elementary OS"
msgstr "Wygląd elementary OS"

#: data/stylesheet.appdata.xml.in:11
msgid ""
"An original Gtk.CSS stylesheet designed specifically for elementary OS and "
"its desktop environment: Pantheon."
msgstr ""
"Oryginalny styl Gtk.CSS zaprojektowany specjalnie dla elementary OS i jego "
"środowiska pulpitu: Pantheon."

#: data/stylesheet.appdata.xml.in:19 data/stylesheet.appdata.xml.in:56
msgid "Improvements:"
msgstr "Ulepszenia:"

#: data/stylesheet.appdata.xml.in:21
msgid "Checkerboard styles are more subtle"
msgstr "Style w szachownicę są bardziej subtelne"

#: data/stylesheet.appdata.xml.in:22
msgid "Set `large-icons` size on headerbars to 24px"
msgstr "Ustaw wielkość `duże-ikony` na paskach nagłówkowych na 24px"

#: data/stylesheet.appdata.xml.in:23
msgid "Adjust header layout and typography in Granite.SimpleSettingsPage"
msgstr "Dostosuj układ nagłówków i typografię w Granite.SimpleSettingsPage"

#: data/stylesheet.appdata.xml.in:24
msgid "Infobars: set correct button margins"
msgstr "Paski informacyjne: poprawnie ustawiono marginesy przycisków"

#: data/stylesheet.appdata.xml.in:25
msgid "Add padding to Gtk.Entry's completion popover"
msgstr "Dodano padding do podpowiedzi Gtk.Entry"

#: data/stylesheet.appdata.xml.in:26
#, fuzzy
#| msgid "Fix accent class for Gtk.Scale"
msgid "Fix labels for scale marks"
msgstr "Naprawiono klasę akcentu dla Gtk.Scale"

#: data/stylesheet.appdata.xml.in:28 data/stylesheet.appdata.xml.in:46
#: data/stylesheet.appdata.xml.in:60
msgid "Fixes:"
msgstr "Poprawki:"

#: data/stylesheet.appdata.xml.in:30
msgid "Correct .title-4 padding in Lists"
msgstr "Poprawiono padding .title-4 w listach"

#: data/stylesheet.appdata.xml.in:31
msgid "Buttons: don't set font weight for children like popovers"
msgstr ""
"Przyciski: nie ustawiają już grubości fontu dla elementów potomnych takich "
"jak popovery"

#: data/stylesheet.appdata.xml.in:42
msgid "New Features:"
msgstr "Nowe funkcje:"

#: data/stylesheet.appdata.xml.in:44
msgid "Support for Gtk4"
msgstr "Wsparcie dla Gtk4"

#: data/stylesheet.appdata.xml.in:48
msgid "Fix titlebar in GNOME Web 42"
msgstr "Naprawiono pasek tytułu w GNOME Web 42"

#: data/stylesheet.appdata.xml.in:49
msgid "Fix font style for bold labels in some languages"
msgstr "Naprawiono styl fontu dla pogrubionych etykiet w niektórych językach"

#: data/stylesheet.appdata.xml.in:50
msgid "ModelButtons: add margin between checks, radios, and labels"
msgstr "ModelButtons: dodano odstęp pomiędzy polami wyboru a ich opisem"

#: data/stylesheet.appdata.xml.in:58
msgid "Improve contrast of horizontal separators in dark style"
msgstr "Zwiększono kontrast pomiędzy poziomymi granicami w stylu ciemnym"

#: data/stylesheet.appdata.xml.in:62
msgid "Use the accent color for highlighting in IBus candidate window"
msgstr "Użyto koloru akcentu do podświetlenia w oknie kandydującym IBus"

#: data/stylesheet.appdata.xml.in:63
msgid "Fix font style for title labels in headerbars"
msgstr "Naprawiono styl fontu dla etykiet tytułowych w paskach nagłówka"

#: data/stylesheet.appdata.xml.in:70
msgid "Support for Hdy.Tabbar"
msgstr "Wsparcie dla Hdy.Tabbar"

#: data/stylesheet.appdata.xml.in:71
msgid "Fix accent class for Gtk.Scale"
msgstr "Naprawiono klasę akcentu dla Gtk.Scale"

#: data/stylesheet.appdata.xml.in:72
msgid "Flat titlebars inherit background color for views and sidebars"
msgstr ""
"Płaskie paski tytułów dziedziczą kolor tła w widokach i w panelach bocznych"

#: data/stylesheet.appdata.xml.in:79
msgid "Complete redesign"
msgstr "Kompletne przeprojektowanie"

#: data/stylesheet.appdata.xml.in:87
msgid "elementary, Inc."
msgstr "elementary, Inc."

